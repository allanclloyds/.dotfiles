# ~/.gitconfig: Allan C. Lloyds <acl@acl.im>

# Do not edit directly; changes will be overwritten
# Generated from <%= File.join(options[:templates], "#{options[:file]}.erb") %>

[user]
  name   = <%= config['name']  %>
  email  = <%= config['email'] %>

[alias]
  br     = branch
  ci     = commit
  co     = checkout
  rb     = rebase
  cp     = cherry-pick
  sb     = show-branch
  sm     = submodule
  st     = status --ignore-submodules=dirty

  a      = add
  b      = branch
  c      = commit
  d      = diff   --ignore-submodules=dirty
  p      = push
  s      = status --ignore-submodules=dirty

[color]
  ui     = auto
  diff   = auto
  status = auto
  branch = auto

[github]
  user   = <%= config['github']['user']  %>
  token  = <%= config['github']['token'] %>

[url "git@github.com:<%= config['github']['user']  %>/"]
  insteadOf = github:
